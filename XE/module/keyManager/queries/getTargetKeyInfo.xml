<query id="getTargetKeyInfo" action="select">
    <tables>
        <table name="keyManager" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="domain" var="domain" />
        <condition operation="equal" column="user_id" var="user_id" />
    </conditions>
</query>