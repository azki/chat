<query id="updateKey" action="update">
    <tables>
        <table name="keyManager" />
    </tables>
    <columns>
    	<column name="keyManager_key" var="keyManager_key" default="" notnull="notnull" minlength="1" maxlength="250" />
        <column name="domain" var="domain" default="" />
        <column name="user_id" var="user_id" default="" />
    </columns>
    <conditions>
        <condition operation="equal" column="keyManager_srl" var="keyManager_srl" default="0" filter="number" notnull="notnull" />
    </conditions>
</query>